<p id="notice"><%= notice %></p>

<h1>Everythings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @everythings.each do |everything| %>
      <tr>
        <td><%= link_to 'Show', everything %></td>
        <td><%= link_to 'Edit', edit_everything_path(everything) %></td>
        <td><%= link_to 'Destroy', everything, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Everything', new_everything_path %>
